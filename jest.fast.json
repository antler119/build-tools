{
  "testEnvironment": "node",
  "testRegex": "/tests/.*-spec.js$",
  "bail": true,
  "testPathIgnorePatterns": [
    "/.tmp/",
    "/tests/bootstrap-spec.js$"
  ],
  "coverageReporters": [
    "none"
  ],
  "collectCoverageFrom": [
    "src/g",
    "src/g-*js"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/build-tools-spec-.*$",
    "/tests/.*\\.js$"
  ]
}